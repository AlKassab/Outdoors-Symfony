<?php

namespace Pi\RsrBundle\Entity;

/**
 * Randonnee
 */
class Randonnee
{
    /**
     * @var integer
     */
    private $idRan;

    /**
     * @var string
     */
    private $lieu;

    /**
     * @var \DateTime
     */
    private $date;

    /**
     * @var string
     */
    private $description;

    /**
     * @var float
     */
    private $prix;

    /**
     * @var float
     */
    private $duree;

    /**
     * @var float
     */
    private $kilometrage;

    /**
     * @var float
     */
    private $altitude;

    /**
     * @var integer
     */
    private $nbrRandonneur;

    /**
     * @var string
     */
    private $type;

    /**
     * @var string
     */
    private $difficulte;

    /**
     * @var string
     */
    private $organisateur;


    /**
     * Get idRan
     *
     * @return integer
     */
    public function getIdRan()
    {
        return $this->idRan;
    }

    /**
     * Set lieu
     *
     * @param string $lieu
     *
     * @return Randonnee
     */
    public function setLieu($lieu)
    {
        $this->lieu = $lieu;

        return $this;
    }

    /**
     * Get lieu
     *
     * @return string
     */
    public function getLieu()
    {
        return $this->lieu;
    }

    /**
     * Set date
     *
     * @param \DateTime $date
     *
     * @return Randonnee
     */
    public function setDate($date)
    {
        $this->date = $date;

        return $this;
    }

    /**
     * Get date
     *
     * @return \DateTime
     */
    public function getDate()
    {
        return $this->date;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return Randonnee
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set prix
     *
     * @param float $prix
     *
     * @return Randonnee
     */
    public function setPrix($prix)
    {
        $this->prix = $prix;

        return $this;
    }

    /**
     * Get prix
     *
     * @return float
     */
    public function getPrix()
    {
        return $this->prix;
    }

    /**
     * Set duree
     *
     * @param float $duree
     *
     * @return Randonnee
     */
    public function setDuree($duree)
    {
        $this->duree = $duree;

        return $this;
    }

    /**
     * Get duree
     *
     * @return float
     */
    public function getDuree()
    {
        return $this->duree;
    }

    /**
     * Set kilometrage
     *
     * @param float $kilometrage
     *
     * @return Randonnee
     */
    public function setKilometrage($kilometrage)
    {
        $this->kilometrage = $kilometrage;

        return $this;
    }

    /**
     * Get kilometrage
     *
     * @return float
     */
    public function getKilometrage()
    {
        return $this->kilometrage;
    }

    /**
     * Set altitude
     *
     * @param float $altitude
     *
     * @return Randonnee
     */
    public function setAltitude($altitude)
    {
        $this->altitude = $altitude;

        return $this;
    }

    /**
     * Get altitude
     *
     * @return float
     */
    public function getAltitude()
    {
        return $this->altitude;
    }

    /**
     * Set nbrRandonneur
     *
     * @param integer $nbrRandonneur
     *
     * @return Randonnee
     */
    public function setNbrRandonneur($nbrRandonneur)
    {
        $this->nbrRandonneur = $nbrRandonneur;

        return $this;
    }

    /**
     * Get nbrRandonneur
     *
     * @return integer
     */
    public function getNbrRandonneur()
    {
        return $this->nbrRandonneur;
    }

    /**
     * Set type
     *
     * @param string $type
     *
     * @return Randonnee
     */
    public function setType($type)
    {
        $this->type = $type;

        return $this;
    }

    /**
     * Get type
     *
     * @return string
     */
    public function getType()
    {
        return $this->type;
    }

    /**
     * Set difficulte
     *
     * @param string $difficulte
     *
     * @return Randonnee
     */
    public function setDifficulte($difficulte)
    {
        $this->difficulte = $difficulte;

        return $this;
    }

    /**
     * Get difficulte
     *
     * @return string
     */
    public function getDifficulte()
    {
        return $this->difficulte;
    }

    /**
     * Set organisateur
     *
     * @param string $organisateur
     *
     * @return Randonnee
     */
    public function setOrganisateur($organisateur)
    {
        $this->organisateur = $organisateur;

        return $this;
    }

    /**
     * Get organisateur
     *
     * @return string
     */
    public function getOrganisateur()
    {
        return $this->organisateur;
    }
}
