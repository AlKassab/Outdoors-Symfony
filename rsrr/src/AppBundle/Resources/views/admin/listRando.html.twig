            <div class="row">
                {% for rando  in randonnees  %}
                    <div class="col-md-4">
                        <div class=" plans pricing-table ">
                            <div class="plans clearfix">
                                <div class="plan">
                                    <div class="plan-header bg-primary">
                                        <div class="title"><span>{{ rando.lieu }}</span></div>
                                        <div class="price"><span class="amount">{{ rando.prix }}</span><span> dt</span></div>
                                        <div class="plan-top-btn text-center"><span class="offer">{{ rando.date|date('Y-m-d') }}</span></div>
                                    </div>
                                    <div class="description">
                                        <div class="p-item">
                                            <p class="plan-item"><b>Kilometrage: </b>{{ rando.kilometrage }} klm</p>
                                        </div>
                                        <div class="p-item">
                                            <p class="plan-item"><b>Altitude: </b> {{ rando.altitude }} cm</p>
                                        </div>
                                        <div class="p-item">
                                            <p class="plan-item"><b> Heure: </b> {{ rando.heure|date('h-m') }} H</p>
                                        </div>
                                        <div class="p-item">
                                            <p class="plan-item"><b> Nbr de places: </b> {{ rando.nbrRandonneur }}</p>
                                        </div>
                                        <div class="text-center plan-bottom-btn p-b-10">
                                            <a class="btn btn-primary" href="{{  path('del', {'id':rando.idRan }) }}"><i class="icon-trash" >  Supprimer </i></a>
                                            <a class="btn btn-primary" href="{{  path('info', {'id':rando.idRan }) }}"><i class="icon-calendar" > Voir details </i></a>
                                        </div>



                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                {% endfor %}
